@article{Cohen2012,
abstract = {We demonstrate that a firm's ability to innovate is predictable, persistent, and relatively simple to compute, and yet the stock market ignores the implications of past successes when valuing future innovation. We show that two firms that invest the exact same in research and development (R{\&}D) can have quite divergent, but predictably divergent, future paths. Our approach is based on the simple premise that while future outcomes associated with R{\&}D investment are uncertain, the past track records of firms may give insight into their potential for future success. We show that a long-short portfolio strategy that takes advantage of the information in past track records earns abnormal returns of roughly 11 percent per year. Importantly, these past track records also predict divergent future real outcomes in patents, patent citations, and new product innovations.},
author = {Cohen, Lauren and Diether, Karl B. and Malloy, Christopher J.},
doi = {10.2139/ssrn.1785454},
file = {:C$\backslash$:/Users/ivers/Google Drive/PhD/Articles/Cohen, L, Diether, K., Malloy, C. (2012) - Misvaluing Innovation.pdf:pdf},
journal = {SSRN Electronic Journal},
title = {{Misvaluing Innovation}},
year = {2012}
}
@article{Da2013,
abstract = {We test a frog-in-the-pan (FIP) hypothesis that predicts investors are inattentive to information arriving continuously in small amounts. Intuitively, we hypothesize that a series of frequent gradual changes attracts less attention than infrequent dramatic changes. Consistent with the FIP hypothesis, we find that continuous information induces strong persistent return continuation that does not reverse in the long run. Momentum decreases monotonically from 5.94{\%} for stocks with continuous information during their formation period to -2.07{\%} for stocks with discrete information but similar cumulative formation-period returns. Higher media coverage coincides with discrete information and mitigates the stronger momentum following continuous information.},
author = {Da, Zhi and Gurun, Umit G. and Warachka, Mitch},
doi = {10.2139/ssrn.2370931},
file = {:C$\backslash$:/Users/ivers/Google Drive/PhD/Articles/Da, Z., Gurun, U.G., Warachka, M. (2013) - Frog in the Pan - Continuous Information and Momentum.pdf:pdf},
journal = {SSRN Electronic Journal},
number = {December},
title = {{Frog in the Pan: Continuous Information and Momentum}},
year = {2013}
}
@article{Daniel2006,
abstract = {The book-to-market effect is often interpreted as evidence of high expected returns on stocks of "distressed" firms with poor past performance. We dispute this interpretation. We find that while a stock's future return is unrelated to the firm's past accounting-based performance, it is strongly negatively related to the "intangible" return, the component of its past return that is orthogonal to the firm's past performance. Indeed, the book-to-market ratio forecasts returns because it is a good proxy for the intangible return. Also, a composite equity issuance measure, which is related to intangible returns, independently forecasts returns.},
author = {Daniel, Kent and Titman, Sheridan},
doi = {10.1111/j.1540-6261.2006.00884.x},
